{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "ignoreCommand": "echo 'Skipping build - static site'",
  "buildCommand": "echo 'No build needed for static site'",
  "outputDirectory": "frontend"
}
